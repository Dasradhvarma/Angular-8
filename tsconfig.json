{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "lib": ["esnext", "dom"],
    "types": ["jest"],
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["src/**/*.ts/"],
  "exclude": ["node_modules", "wwwroot/lib", "dist"],
"angularCompilerOptions": {
"fullTemplateTypeCheck": true,
"strictInjectionParameters": true
}
}
